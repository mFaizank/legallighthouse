<%= simple_form_for @quote, url: user_quotes_path(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :agreement, as: :file, label: 'Retainer Agreement' %>
    <%= f.input :fee %>
    <%= f.input :advance %>
    <%= f.input :completion_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Send" %>
  </div>
<% end %>
